{
  "name": "surveyor",
  "version": "1.0.0",
  "description": "A mapbox-gl-js front end for surveys using Survey123",
  "main": "index.js",
  "scripts": {
    "watch": "watchify src/main.js -t babelify -o public/bundle.js -v",
    "build": "browserify src/main.js -t babelify | uglifyjs > public/bundle.js",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d public",
    "test": "mocha --compilers js:babel-core/register"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CityOfDetroit/surveyor.git"
  },
  "dependencies": {
    "mapbox-gl": "^0.39.1",
    "lodash": "^4.17.4"
  },
  "devDependencies": {
    "babel-preset-env": "^1.6.0",
    "babel-preset-es2015": "^6.24.1",
    "babelify": "^7.3.0",
    "gh-pages": "^1.0.0",
    "should": "^11.2.1"
  },
  "author": "",
  "license": "ISC"
}
